{
  "name": "dalton-walsh-cv",
  "version": "1.0.0",
  "description": "Static CV website for Dalton Walsh",
  "main": "index.html",
  "scripts": {
    "sass": "sass css/style.scss css/style.css",
    "sass:watch": "sass --watch css/style.scss:css/style.css",
    "serve": "live-server --port=3000 --open=/index.html",
    "dev": "npm run sass:watch & npm run serve",
    "build": "sass css/style.scss css/style.css --style compressed"
  },
  "keywords": [
    "cv",
    "resume",
    "static",
    "sass"
  ],
  "author": "Dalton Walsh",
  "license": "MIT",
  "devDependencies": {
    "live-server": "^1.2.2",
    "sass": "^1.70.0"
  }
}
